name: dockercompose13396362521259423529
services:
  elasticsearch:
    container_name: elasticsearch
    environment:
      ES_JAVA_OPTS: -Xms512m -Xmx512m
      cluster.name: f1-cluster
      discovery.type: single-node
      node.name: es-node
      xpack.security.enabled: "false"
      xpack.security.enrollment.enabled: "false"
    image: docker.elastic.co/elasticsearch/elasticsearch:8.11.3
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 9200
        published: "9200"
        protocol: tcp
    volumes:
      - type: volume
        source: elastic_data
        target: /usr/share/elasticsearch/data
        volume: {}
  formulaone:
    build:
      context: C:\Users\ivasc\source\repos\CSharp\FormulaOne
      dockerfile: Backend/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
        LAUNCHING_FROM_VS: "true"
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: FormulaOne
      target: base
    container_name: f1_api
    depends_on:
      elasticsearch:
        condition: service_started
        required: true
      postgres:
        condition: service_healthy
        required: true
      redis:
        condition: service_started
        required: true
      seq:
        condition: service_started
        required: true
    entrypoint:
      - dotnet
      - --roll-forward
      - Major
      - /VSTools/DistrolessHelper/DistrolessHelper.dll
      - --wait
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:8080
      ConnectionStrings__DefaultConnection: Host=postgres;Port=5432;Database=FormulaOne;Username=postgres;Password=123
      ConnectionStrings__Redis: redis:6379
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      Elastic__Url: http://elasticsearch:9200
      NUGET_FALLBACK_PACKAGES: /.nuget/fallbackpackages
      Seq__ServerUrl: http://seq:80
    image: formulaone:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /.nuget/packages --additionalProbingPath /.nuget/fallbackpackages  "/app/bin/Debug/net9.0/FormulaOne.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: dotnet --roll-forward Major /VSTools/DistrolessHelper/DistrolessHelper.dll --stop dotnet
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: C:\Users\ivasc\AppData\Roaming\Microsoft\UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\AppData\Roaming\Microsoft\UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\AppData\Roaming\ASP.NET\Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\AppData\Roaming\ASP.NET\Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\source\repos\CSharp\FormulaOne\Backend
        target: /app
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\source\repos\CSharp\FormulaOne
        target: /src
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\vsdbg\vs2017u5
        target: /remote_debugger
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\ivasc\.nuget\packages
        target: /.nuget/packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages
        target: /.nuget/fallbackpackages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\MSBuild\Sdks\Microsoft.Docker.Sdk\tools\linux-x64\net6.0
        target: /VSTools
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Program Files\Microsoft Visual Studio\2022\Community\Common7\IDE\CommonExtensions\Microsoft\HotReload
        target: /HotReloadAgent
        read_only: true
        bind:
          create_host_path: true
  kibana:
    container_name: kibana
    depends_on:
      elasticsearch:
        condition: service_started
        required: true
    environment:
      ELASTICSEARCH_HOSTS: http://elasticsearch:9200
      XPACK_SECURITY_ENABLED: "false"
    image: docker.elastic.co/kibana/kibana:8.11.3
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 5601
        published: "5601"
        protocol: tcp
  postgres:
    container_name: f1_postgres
    environment:
      POSTGRES_DB: FormulaOne
      POSTGRES_PASSWORD: "123"
      POSTGRES_USER: postgres
    healthcheck:
      test:
        - CMD-SHELL
        - pg_isready -U postgres -d FormulaOne
      timeout: 5s
      interval: 5s
      retries: 5
    image: postgres:16
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5433"
        protocol: tcp
    volumes:
      - type: volume
        source: postgres_data
        target: /var/lib/postgresql/data
        volume: {}
  redis:
    container_name: f1_redis
    image: redis:7
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 6379
        published: "6379"
        protocol: tcp
  seq:
    container_name: f1_seq
    environment:
      ACCEPT_EULA: "Y"
    image: datalust/seq:2024.3
    networks:
      f1_network: null
    ports:
      - mode: ingress
        target: 80
        published: "5341"
        protocol: tcp
    volumes:
      - type: volume
        source: seq_data
        target: /data
        volume: {}
networks:
  f1_network:
    name: dockercompose13396362521259423529_f1_network
    driver: bridge
volumes:
  elastic_data:
    name: dockercompose13396362521259423529_elastic_data
  postgres_data:
    name: dockercompose13396362521259423529_postgres_data
  seq_data:
    name: dockercompose13396362521259423529_seq_data